{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/generate-affirmation",
      "dest": "/api/generate-affirmation.js"
    },
    {
      "src": "/generate-daily-affirmation",
      "dest": "/api/generate-daily-affirmation.js"
    },
    {
      "src": "/generate-relationship-affirmation",
      "dest": "/api/generate-relationship-affirmation.js"
    }
  ]
}
